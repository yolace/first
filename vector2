#include < iostream>
#include<vector>
using namespace std;
int main()
{    
	int n;
	cout << "请键入向量的元素量：";
	cin >> n;https://github.com/yolace/first
	 vector<float> a(n, 0);       //对其中部分元素进行输入
	 vector<float> b(n, 0);
	int i;
	cout << "请键入向量1的元素"<<endl;
	for (i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	cout << "请键入向量2的元素"<<endl;
	float dot_product = 0; //设定向量点积
	for (i = 0; i < n; i++)
	{
		cin >> b[i];
		dot_product += a[i] * b[i];
	}
	
	cout << dot_product;
	
      return 0;
}
